    <div class="main-container">
        <section class="text-center space--md">
            <div class="container">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="typed-headline">
                            <span class="h2 inline-block">지윤이가</span>
                            <span class="h2 inline-block typed-text typed-text--cursor color--primary"
                                data-typed-strings="왕이다,공주다,총장이다,대장이다,오야붕이다,프락치다">
                            </span>
                        </div>
                    </div>
                </div>
                <!--end of row-->
            </div>
            <!--end of container-->
        </section>
        <section class="space--sm">
            <div class="container">
                <div class="row">
                    <div class="col-sm-10 col-md-8 col-sm-offset-1 col-md-offset-2">
                        <div class="masonry masonry-blog-list">
                            <div class="masonry-filter-container text-center">
                                <span>Category:</span>
                                <div class="masonry-filter-holder">
                                    <div class="masonry__filters" data-filter-all-text="all"></div>
                                </div>
                            </div>
                            <table class="table table-hover" style="margin: 0 0 0 0">
                                <thead>
                                    <tr>
                                        <th scope="col" class="col-sm-1" style="text-align: center">번호</th>
                                        <th scope="col" class="col-sm-6" style="text-align: center">제목</th>
                                        <th scope="col" class="col-sm-1">작성자</th>
                                        <th scope="col" class="col-sm-3" style="text-align: center">작성일</th>
                                        <th scope="col" class="col-sm-2">조회수</th>
                                    </tr>
                                </thead>
                            </table>
                            <hr>

                            <div class="masonry__container">
                                    
                                <article class="masonry__item" data-masonry-filter="자유게시판">
                                    <% if(boards){ %>
                                    <% for(var i=boards.length-1;i>=0; i--){%>
                                    <% if(boards[i].category == '자유게시판'){%>
                                    <section class="space--xs blog-article-wide" style="margin:0 0 0 0;padding:0 0 0 0">
                                        <div class="container" style="text-align: left">
                                            <div class="row">
                                                <div class="cta cta--horizontal text-center-xs">
                                                    <div class="col-sm-1">
                                                        <h4>
                                                            <%= i+1%>
                                                        </h4>
                                                    </div>
                                                    <div class="col-sm-6">
                                                        <p class="lead">
                                                            <%= boards[i].category %> <a href="/show/<%= boards[i]._id %>">
                                                                <%= boards[i].title %></a>
                                                        </p>
                                                    </div>
                                                    <div class="col-sm-1">
                                                        <h4>
                                                            <%= boards[i].author%>
                                                        </h4>
                                                    </div>
                                                    <div class="col-sm-3" style="text-align: center">
                                                        <h4>
                                                            <%= boards[i].created_at%>
                                                        </h4>
                                                    </div>
                                                    <div class="col-sm-1" style="text-align: center">
                                                        <h4>
                                                            <%= boards[i].hits%>
                                                        </h4>
                                                    </div>
                                                </div>
                                            </div>
                                            <!--end of row-->
                                        </div>
                                        <!--end of container-->
                                    </section>
                                    <%}}}%>
                                </article>

                                <article class="masonry__item" data-masonry-filter="코딩게시판">
                                    
                                        <% if(boards){ %>
                                        <% for(var i=boards.length-1;i>=0; i--){%>
                                        <% if(boards[i].category == '코딩게시판'){%>
                                        <section class="space--xs blog-article-wide" style="margin:0 0 0 0;padding:0 0 0 0">
                                            <div class="container" style="text-align: left">
                                                <div class="row">
                                                    <div class="cta cta--horizontal text-center-xs">
                                                        <div class="col-sm-1">
                                                            <h4>
                                                                <%= i+1%>
                                                            </h4>
                                                        </div>
                                                        <div class="col-sm-6">
                                                            <p class="lead">
                                                                <%= boards[i].category %> <a href="/coding/<%= boards[i]._id %>">
                                                                    <%= boards[i].title %></a>
                                                            </p>
                                                        </div>
                                                        <div class="col-sm-1">
                                                            <h4>
                                                                <%= boards[i].author%>
                                                            </h4>
                                                        </div>
                                                        <div class="col-sm-3" style="text-align: center">
                                                            <h4>
                                                                <%= boards[i].created_at%>
                                                            </h4>
                                                        </div>
                                                        <div class="col-sm-1" style="text-align: center">
                                                            <h4>
                                                                <%= boards[i].hits%>
                                                            </h4>
                                                        </div>
                                                    </div>
                                                </div>
                                                <!--end of row-->
                                            </div>
                                            <!--end of container-->
                                        </section>
                                        <%}}}%>
                                    </article>
                                
                                    <article class="masonry__item" data-masonry-filter="고양이게시판">
                                    
                                            <% if(boards){ %>
                                            <% for(var i=boards.length-1;i>=0; i--){%>
                                            <% if(boards[i].category == '고양이게시판'){%>
                                            <section class="space--xs blog-article-wide" style="margin:0 0 0 0;padding:0 0 0 0">
                                                <div class="container" style="text-align: left">
                                                    <div class="row">
                                                        <div class="cta cta--horizontal text-center-xs">
                                                            <div class="col-sm-1">
                                                                <h4>
                                                                    <%= i+1%>
                                                                </h4>
                                                            </div>
                                                            <div class="col-sm-6">
                                                                <p class="lead">
                                                                    <%= boards[i].category %> <a href="/show/<%= boards[i]._id %>">
                                                                        <%= boards[i].title %></a>
                                                                </p>
                                                            </div>
                                                            <div class="col-sm-1">
                                                                <h4>
                                                                    <%= boards[i].author%>
                                                                </h4>
                                                            </div>
                                                            <div class="col-sm-3" style="text-align: center">
                                                                <h4>
                                                                    <%= boards[i].created_at%>
                                                                </h4>
                                                            </div>
                                                            <div class="col-sm-1" style="text-align: center">
                                                                <h4>
                                                                    <%= boards[i].hits%>
                                                                </h4>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <!--end of row-->
                                                </div>
                                                <!--end of container-->
                                            </section>
                                            <%}}}%>
                                        </article>
                                <!--end item-->
                                <div class="text-center">

                                    <!-- <div>
                <select name="category">
                    <option value="코딩게시판">코딩게시판</option>
                    <option value="자유게시판">자유게시판</option>
                    <option value="고양이게시판">고양이게시판</option>
                    <option value="전체게시판" selected="selected">전체게시판</option>
                  </select>
            </div> -->


                                    <div style="text-align:right">
                                        <a href="/writing"><button class="btn btn-dark">글쓰러가깅 </button>
                                    </div>


                                    <!-- 페이지 수..ㅎ -->
                                    <section>
                                        <div class="container">
                                            <div class="row">
                                                <div class="col-sm-12">
                                                    <div class="pagination">
                                                        <a class="pagination__prev" href="#" title="Previous Page">&laquo;</a>
                                                        <ol>
                                                            <li>
                                                                <a href="#">1</a>
                                                            </li>
                                                            <li>
                                                                <a href="#">2</a>
                                                            </li>
                                                            <li>
                                                                <a href="#">3</a>
                                                            </li>
                                                            <li>
                                                                <a href="#">4</a>
                                                            </li>
                                                        </ol>
                                                        <a class="pagination__next" href="#" title="Next Page">&raquo;</a>
                                                    </div>
                                                </div>
                                            </div>
                                            <!--end of row-->
                                        </div>
                                        <!--페이지 수 끝-->
                                    </section>
                                </div>
                            </div>
                            <script src="/js/jquery-3.1.1.min.js"></script>
                            <script src="/js/flickity.min.js"></script>
                            <script src="/js/easypiechart.min.js"></script>
                            <script src="/js/parallax.js"></script>
                            <script src="/js/typed.min.js"></script>
                            <script src="/js/datepicker.js"></script>
                            <script src="/js/isotope.min.js"></script>
                            <script src="/js/ytplayer.min.js"></script>
                            <script src="/js/lightbox.min.js"></script>
                            <script src="/js/granim.min.js"></script>
                            <script src="/js/jquery.steps.min.js"></script>
                            <script src="/js/countdown.min.js"></script>
                            <script src="/js/twitterfetcher.min.js"></script>
                            <script src="/js/spectragram.min.js"></script>
                            <script src="/js/smooth-scroll.min.js"></script>
                            <script src="/js/scripts.js"></script>
</body>

</html>
